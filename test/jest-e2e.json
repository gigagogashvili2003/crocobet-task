{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@app/users/(.*)": "<rootDir>/../libs/users/src/$1",
    "@app/users": "<rootDir>/../libs/users/src",
    "@app/auth/(.*)": "<rootDir>/../libs/auth/src/$1",
    "@app/auth": "<rootDir>/../libs/auth/src",
    "@app/collections/(.*)": "<rootDir>/../libs/collections/src/$1",
    "@app/collections": "<rootDir>/../libs/collections/src",
    "@app/common/(.*)": "<rootDir>/../libs/common/src/$1",
    "@app/common": "<rootDir>/../libs/common/src",
    "@app/utils/(.*)": "<rootDir>/../libs/utils/src/$1",
    "@app/utils": "<rootDir>/../libs/utils/src",
    "@app/redis/(.*)": "<rootDir>/../libs/redis/src/$1",
    "@app/redis": "<rootDir>/../libs/redis/src",
    "@app/sessions/(.*)": "<rootDir>/../libs/sessions/src/$1",
    "@app/sessions": "<rootDir>/../libs/sessions/src",
    "@app/books/(.*)": "<rootDir>/../libs/books/src/$1",
    "@app/books": "<rootDir>/../libs/books/src",
    "@app/db/(.*)": "<rootDir>/../libs/db/src/$1",
    "@app/db": "<rootDir>/../libs/db/src"
  }
}